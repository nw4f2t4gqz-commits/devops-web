# Copy this file to .env and fill values for your environment before running docker-compose
# Minimal required values for admin web and email
SECRET_KEY=change-me-to-a-secure-random-value
ADMIN_USER=admin
ADMIN_PASS=change-me
SMTP_HOST=smtp.office365.com
SMTP_PORT=587
SMTP_USER=your-smtp-user@example.com
SMTP_PASS=your-smtp-password
EMAIL_TO=you@example.com
# Optional: redis (for production rate-limiter)
# Optional: redis (for production rate-limiter) - not required for this project
# REDIS_URL=redis://redis:6379/0
# Optional: override Gunicorn workers
GUNICORN_WORKERS=2
